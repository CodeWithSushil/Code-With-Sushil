{
    "require": {
        "slim/slim": "^4.13",
        "php-di/php-di": "^7.0",
        "twig/twig": "^3.9",
        "vlucas/phpdotenv": "^5.6",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.1",
        "symfony/cache": "^7",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "monolog/monolog": "^3.6"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
